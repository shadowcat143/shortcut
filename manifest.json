{
    "name": "shortcut",
    "description": "a chrome Extension takes you to my website with 2 steps!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html"
    }
  }